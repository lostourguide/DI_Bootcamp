#Exercise 2: Whatâ€™s your favorite book ?
def favorite_book(title):
    """"Prints a message about the user's favorite book.

    Args:
    title(str):The title of the book."""

    print(f"One of my favorite books is {title}.")

favorite_book("Alice in Wonderland")


#Exercise 3 : Some Geography
def describe_city(city, country="Iceland"):
    """Prints a sentence describing the location of the city."""
    print(f"{city} is in {country}")

describe_city("Reykjavik") 


#Exercise 4 : Random
def compare_random_number(user_number):
    print("Please enter a number btween 1 and 100.")
    return

generated_number = import(random.int(1, 100))

if user_number == generated_number:
    print(f"Success! Both numbers are{user_number}.")
else:
    print(f"Fail! Your number: {user_number}, generated number: {generated_number}")

user_input = int(input("enter a number between 1 and 100:"))

compare_random_number(user_input)


